{
  "env": {
    "node": true,
  },
  "globals": {
    "angular": true,
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "space-infix-ops": 2,
    "no-trailing-spaces": 2,
    "valid-jsdoc": 2,
    "require-jsdoc": 2,
    "comma-spacing": 2,
    "keyword-spacing": 2,
    "spaced-comment": 2
  }
}
